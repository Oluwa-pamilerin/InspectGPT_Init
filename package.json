{
  "name": "inspectgpt",
  "displayName": "InspectGPT",
  "publisher": "OTimSobiyede2",
  "description": "Your extension description here",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:inspectgpt.openWebview"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.logFolderContents",
        "title": "Log Folder Contents"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.logFolderContents",
          "when": "editorFocus"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.81.0",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@google-ai/generativelanguage": "^1.1.0",
    "axios": "^1.5.0",
    "bard-ai": "^2.0.3",
    "dotenv": "^16.3.1",
    "fast-xml-parser": "^4.2.7",
    "node-fetch": "^3.3.2",
    "xml-parser": "^1.2.1"
  }
}
